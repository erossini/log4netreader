  <data name="textBox1.Text" xml:space="preserve">
    <value>Add this section to target application in log4net appenders list       

&lt;appender name="RemotingAppender" type="log4net.Appender.RemotingAppender" &gt;
  &lt;sink value="tcp://10.10.220.232:8085/LoggingSink" /&gt; &lt;!-- The remoting URL to the remoting server object --&gt;
  &lt;lossy value="false" /&gt;                               &lt;!-- Send all events, do not discard events when the buffer is full --&gt;
  &lt;bufferSize value="0" /&gt;                              &lt;!-- The number of events to buffer before sending --&gt;
  &lt;onlyFixPartialEventData value="true" /&gt;              &lt;!-- Do not store event data that is slow to generate --&gt;
  &lt;layout type="log4net.Layout.XmlLayoutSchemaLog4j"&gt;
    &lt;locationInfo value="true" /&gt;
  &lt;/layout&gt;
&lt;/appender&gt;</value>
